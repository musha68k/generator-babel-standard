{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "watch": "watchy -w src/**.js -- npm run build",
    "watch-test": "watchy -w src/**.js,test/**.js -- npm test",
    "pretest": "npm run build",
    "test": "standard && mocha --compilers js:babel/register",
    "prepublish": "npm test"
  },
  "keywords": [
    ""
  ],
  "dependencies": {},
  "devDependencies": {
    "babel": "*",
    "mocha": "*",
    "standard": "*",
    "watchy": "*"
  },
  "standard": {
    "ignore": [
      "dist/**"
    ]
  }
}
